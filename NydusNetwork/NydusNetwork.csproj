<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Description>NydusNetwork provides raw access to the StarCraft II client using protobuf. Designed for the Abathur Framework but can be used separately.</Description>
    <Company>AdequateSource</Company>
    <Authors>SchmidtGit</Authors>
    <PackageProjectUrl>https://adequatesource.com/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/schmidtgit/SC2Abathur</RepositoryUrl>
    <PackageTags>StarCraft</PackageTags>
    <NeutralLanguage></NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.25.1" />
  </ItemGroup>

</Project>
